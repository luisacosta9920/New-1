app.checkout = {

	/**
	 * Función que fija el bloque resumen del checkout siempre que quepa
	 */
	summaryFixed : function (summaryToTopOriginal, colHeight) 
	{
		// Distancia del scroll hasta la parte superior de la pantalla
	    scrollTop     = $(window).scrollTop();

	    // Altura de la pantalla
	    viewportHeight = jQuery(window).height();

	    // Altura de la cabecera
	    headerHeight = jQuery('.js-main-header').outerHeight(true);

	    // Altura del bloque resumen de compra
	    summaryHeight = jQuery('.js-summary').outerHeight(true);

	    // Anchura del bloque resumen de compra
	    summaryWidth = jQuery('.js-summary').outerWidth(true);

	    // Distancia del bloque resumen de compra hasta la parte superior de la pantalla:
	    scrollSummaryToTop = summaryToTopOriginal - scrollTop;

	    result = viewportHeight - headerHeight - summaryHeight;

	    // Si el bloque resumen de compra cabe:
	    if (result > 0) {
	        // Anclo
	        if (scrollSummaryToTop <= 0) {
	            jQuery('.js-summary').removeClass('checkout__summary--fixed-bottom');
	            jQuery('.js-summary').addClass('checkout__summary--fixed');
	            jQuery('.js-summary').css('width', summaryWidth);
	            
	            // Anclo por la parte de abajo
	            if ( scrollSummaryToTop <= (summaryHeight - colHeight) ) {
	                jQuery('.js-summary').addClass('checkout__summary--fixed-bottom');
	                jQuery('.js-summary').css('top', 'auto');
	            }
	            // No anclo por la parte de abajo
	            else {
	                jQuery('.js-summary').removeClass('checkout__summary--fixed-bottom');
	                jQuery('.js-summary').css('top', 0);
	            }
	        }
	        // No anclo
	        else {
	            jQuery('.js-summary').removeClass('checkout__summary--fixed');
	            jQuery('.js-summary').css('width', 'auto');
	        }	            
	    }
	    // END - Si el bloque resumen de compra cabe
	    else {
	        jQuery('.js-summary').removeClass('checkout__summary--fixed');
	        jQuery('.js-summary').css('width', 'auto');
	    }
	}

};

