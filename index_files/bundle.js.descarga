            initActionShops: function(){
                $(".js-map-shop").on("click",function(e){
                    e.preventDefault();

                    var id_shop=$(this).parent().parent().parent().data("id");
                  
                    var numShops = _shops.length;
                    for(var i=0; i<numShops;i++) {
                        
                        if(id_shop==_shops[i].id){
                            var shopItem = _shops[i];


                            _map.setCenter(shopItem.marker.position);
                            _map.setZoom(shopItem.zoom);

                            $("html, body").animate({ scrollTop: $(_mapId).offset().top }, 1000);
                            break;
                        }
                    }

                    
                });
            },

            updateShopsMap: function(val){

                var $shopsList = $(".shops-list");

                var numShops = _shops.length;
                var shop;

                var counter=0;
                $shopsList.empty();
                for(var i=0; i<numShops;i++) {  
                    shop = _shops[i];

                    


                    if(parseFloat(shop.lng)>=parseFloat(_p1lng) && parseFloat(shop.lng)<=parseFloat(_p2lng) && parseFloat(shop.lat)>=parseFloat(_p1lat) && parseFloat(shop.lat)<=parseFloat(_p2lat)){
                        shop.active=true;


                        if(_filterDistributors && _filterDealers){
                            shop.active=true;
                        }else{
                            if (_filterDistributors && !shop.distributor){
                                shop.active = false;
                            }
                            if (_filterDealers && shop.distributor){
                                shop.active = false;
                            }
                        }

                        if(shop.active===true){


                            $shopsList.append(this.htmlItemList(shop, counter));
                            counter++;
                            shop.marker.setMap(_map);
                            _markers.push(shop.marker);
                        }
                    }else{
                        shop.active=false;
                    }
                }

                if(counter>0){
                   
                    this.loadList(1);
                    this.renderList();
                    checkLoadShops();
                }else{
                    this.loadList(1);
                }

                
                _block_map=false;

                
            },


            updateShops: function(){

                this.recalculateDistance();
                var numShops = _shops.length;
                var shop;
                var total=0;

                for(var i=0; i<numShops;i++) {
                    shop = _shops[i];

                    shop.active = true;
                    if(_filterDistributors && _filterDealers){
                        shop.active=true;
                    }else{
                        if (_filterDistributors && !shop.distributor){
                            shop.active = false;
                        }
                        if (_filterDealers && shop.distributor){
                            shop.active = false;
                        }
                    }

                    
                    if (_filterSearch && !isShopInSearch(shop)){
                        shop.active = false;
                    }

                    if(shop.active)
                        total++;
                }



                this.loadList();

                _block_map=false;

                return total;
            },
            
            recalculateDistance: function()
            {
                var numShops = _shops.length;
                for(var i=0; i<numShops;i++) {
                    shop = _shops[i];
                    shop.distance = getDistanceFromLatLonInKm(_map.getCenter().lat(), _map.getCenter().lng(), shop.lat, shop.lng); 
                    shop.distanceTxt = parseInt(shop.distance)+ ' kms'; 
                }
                orderShopsByNearly();
            },

            loadMoreShops: function() {
                var counter = 0;

                $(".shops-list__item").each(function (index)
