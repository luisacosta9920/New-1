                _$btnSend = $('.js-newsletter-ok');
                _$interests = $('.js-newsletter-option');
                _$messageSend = $('#messageInterests');
                _urlToSave = _$btnSaveEmail.data('url');


                _$btnSaveEmail.on('click', $.proxy(this.saveEmail, this));
                _$btnSend.on('click', $.proxy(this.validateNewsletter, this));

            },

            saveEmail: function(){
                _email = $("#newsletter-email").val();
                _user_type = $('input[name=newsletter_user]').val();

                var _privacy = $("#legal-privacy").val();
                $('.js-pinfo-error').hide();
                $('.js-pinfo-policy').hide();

                $('.js-pinfo-alert').hide();

                if (_email === ''){
                    $('.js-pinfo-error').show();
                    $('.js-pinfo-alert').show();
                }
                if(!$("#newsletter").find('input[name=legal-privacy]').is(":checked")){
                    $('.js-pinfo-policy').show();
                    $('.js-pinfo-alert').show();
                }

                if(!$("#newsletter").find('input[name=legal-legal]').is(":checked")){
                    $('.js-pinfo-policy').show();
                    $('.js-pinfo-alert').show();
