            var valid = $(".js-newsletter-option:checked").length;

            if (valid) {
                if (_$messageSend.is(":visible")) {
                    setTimeout(function () {
                        _$messageSend.slideToggle(300);
                    }, 200);
                }
            }

            return valid;
