    function addColorSelect() {
      //añadimos la selección
      colors_select.push(aux_color);


      //preparamos el slider de colores
      var slider = document.getElementById('slider-color');

      var paso_slider = Math.round(100 / colors_select.length);

      pasos_slider = [];
      classes_slider = [];
      connects_slider = [];

      $.each(colors_select, function (index, v_color) {
        pasos_slider.push(paso_slider * (index + 1));
        connects_slider.push(true);
        classes_slider.push('c-' + v_color.id + '-color');
      });

      //ultimo no tiene color
      connects_slider.push(false);

      if ($('.noUi-horizontal').length > 0) {
        slider.noUiSlider.destroy()
      }

      noUiSlider.create(slider, {
        start: pasos_slider,
        connect: connects_slider,
        range: {
          'min': [0],
          'max': [100]
        }
      });

      $('#slider-color').outerHeight($('.js-image-room').outerHeight());

      $(window).resize(function (){
        $('#slider-color').outerHeight($('.js-image-room').outerHeight());
      });

      var connect = slider.querySelectorAll('.noUi-connect');
      var classes = classes_slider;

      for (var i = 0; i < connect.length; i++) {
        connect[i].classList.add(classes[i]);
      }

      var origins = slider.getElementsByClassName('noUi-origin');
      origins[origins.length - 1].setAttribute('disabled', true);


      $.each(colors_select, function (index, v_color) {
        $('.c-' + v_color.id + '-color').css('background', v_color.hexadecimal);
      });


      //añadimos un color al listado final
      $.ajax({
        type: "POST",
        async: false,
        url: _base_url + 'endpoints/addColor',
        data: "id=" + aux_color.id + "&colors=" + JSON.stringify(colors_select),
        dataType: 'json',
        success: function (json) {

          if (json.code == 'OK') {

            $('.js-related-products-carousel').html(json.html_products);
            $('.js-content-colors-select').append(json.html_color);

            $(".js-related-products-carousel").flickity({
              cellAlign: "left",
              contain: true,
              groupCells: true,
              pageDots: false,
              wrapAround: true
            });

            $('.js-delete-color').click(function () {
              var id_color = $(this).attr('data-id');
        
        
              aux_colors_select = [];
              $.each(colors_select, function (index, value) {
                if (value.id != id_color)
                  aux_colors_select.push(value);
              });
              colors_select = aux_colors_select;
        
        
              //refrescamos los productos
              $.ajax({
                type: "POST",
                async: false,
                url: _base_url + 'endpoints/addColor',
                data: "id=" + id_color + "&colors=" + JSON.stringify(colors_select),
                dataType: 'json',
                success: function (json) {
        
                  if (json.code == 'OK') {
        
                    $('.js-related-products-carousel').html(json.html_products);
        
                    $('.js-color-select-' + id_color).fadeOut('fast', function () {
                      $('.js-color-select-' + id_color).remove();
                    });
        
                  }
        
                }
              });
        
        
            });


          }

        }
      });


    }



    // End //?paso a paso

    // Comprobar seccion on scroll
    $.fn.isInViewport = function () {
      var elementTop = $(this).offset().top;
      var elementBottom = elementTop + $(this).outerHeight();

      var viewportTop = $(window).scrollTop();
      var viewportBottom = viewportTop + $(window).height();

      return elementBottom > viewportTop && elementTop < viewportBottom;
    };

