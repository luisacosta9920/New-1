        if (
          e.keyCode == app.search.keycodeCTRL ||
          e.keyCode == app.search.keycodeCMD ||
          e.keyCode == app.search.keycodeCMD_RIGHT
        )
          app.search.ctrlDown = true;

        // Search is opened
        if (app.search.searchh.is(":visible")) {
          if (e.which == app.search.keycodeESC) {
            app.search.searchClose();
            app.search.searchInput.trigger("blur");
          } else if (
            (e.which >= 48 && e.which <= 90) ||
            (e.which >= 96 && e.which <= 105) ||
            (e.shiftKey === true &&
              ((e.which >= 48 && e.which <= 90) ||
                (e.which >= 96 && e.which <= 105)))
          ) {
            app.search.searchInput.trigger("focus");
            // app.search.searchAction();
          }
        } else {
          // Search is closed
          if (
            app.search.ctrlDown &&
            (e.keyCode == app.search.keycodeV ||
              e.keyCode == app.search.keycodeC ||
              e.keyCode == app.search.keycodeR)
          ) {
          } else {
            var inputType = e.target.nodeName.toLowerCase();
            if (
              inputType != "input" &&
              inputType != "textarea" &&
              inputType != "select"
            ) {
              if (
                (e.which >= 48 && e.which <= 90) ||
                (e.which >= 96 && e.which <= 105) ||
                (e.shiftKey === true &&
                  ((e.which >= 48 && e.which <= 90) ||
                    (e.which >= 96 && e.which <= 105)))
              ) {
                app.search.searchInput.trigger("focus");

                // grab the key pressed ( String.fromCharCode(e.which) )
                // and insert it into app.search.searchInput.
                // then,set the cursor to the end of app.search.searchInput.
                searchValPrev = app.search.searchInput.val();
                app.search.searchInput.val(
                  String.fromCharCode(e.which).toLowerCase()
                );

                window.setTimeout(function() {
                  app.search.searchInput.setCursorPosition(
                    app.search.searchInput.val().length
                  );
                }, 0);

                searchVal = app.search.searchInput.val();
                app.search.searchOpen();
              }
            }
          }
        }
      })
      .on("keyup", function(e) {
        if (
          e.keyCode == app.search.keycodeCTRL ||
