  function validateEmail(email){
        var re = /\S+@\S+\.\S+/;
        return re.test(email);
    }


    $(".user-email-download-module-accept-btn").on("click", function (e) {
      e.preventDefault();

      var module_id = window.download_module_id;

      var email = $('.user-email-download-module-'+module_id).val();

      if(checkUserEmailDownloadModuleTerms()) processEmailDownloadModuleData(email, module_id);
      else showErrorUserEmailDownloadModuleTerms();
    });

    $('.user-email-download-module').blur(function() {
      checkUserEmailModule($(this).val());
    });

    function checkUserEmailModule(email){
      if(email != '' && validateEmail(email)){
        $('#user-email-download-module-accept-btn-'+window.download_module_id).show();
        $('#user-email-download-module-email-error-'+window.download_module_id).hide();
      }else if(email != '' && !validateEmail(email)){
        $('#user-email-download-module-accept-btn-'+window.download_module_id).hide();
        $('#user-email-download-module-email-error-'+window.download_module_id).show();
      }
    }


    function processEmailDownloadModuleData(email,module_id){
      hideErrorUserEmailDownloadModuleTerms();
      var url = window.location.href;

      $.ajax({
        type: "POST",
        async: false,
        url: _base_url + 'endpoints/save_lead_email_download_file',
        data: "email=" + email +'&module_id='+module_id+'&url='+url,
        dataType: 'json',
        success: function (json) {

          if (json.code == 'OK' && json.url) {

              downloadURIFile(json.url);

             

              $('#user-email-download-module-email-error-link').hide();

              $('.close-add-email-module').click();

              $('#user-email-download-module').val('');

              $('#user-email-download-module-accept-btn').hide();

              $('#user-email-download-module-email-error').hide();

          }else{
            $('#user-email-download-module-email-error-link').show();
          }

        }
      });
    }

    var timer;
    var timeout = 750;

    $('#user-email-download-module').keyup(function(){
        clearTimeout(timer);
        if ($('#user-email-download-module').val) {
            timer = setTimeout(function(){
                
                var email = $("#user-email-download-module").val();
                checkUserEmailDownloadModule(email);

            }, timeout);
        }
    });

    function downloadURIFile(uri)
    {
        var link = document.createElement("a");
        // If you don't know the name or want to use
        // the webserver default set name = ''
        link.setAttribute('download', 'tkrom-descarga-modulo');
        link.href = uri;
        document.body.appendChild(link);
        link.click();
        link.remove();
    }

    function checkUserEmailDownloadModule(email){
      if(email != '') checkUserEmailModule(email);
    }

    function checkUserEmailDownloadModuleTerms(){
      return $('#module-download-email-legal-privacy-'+window.download_module_id).is(":checked");
    }

    function showErrorUserEmailDownloadModuleTerms(){
      $('#user-email-download-module-email-terms-error').show();
    }

    function hideErrorUserEmailDownloadModuleTerms(){
      $('#user-email-download-module-email-terms-error').hide();
    }

    function addRecaptchaToken (token) {
  
      $("<input />").attr("type", "hidden")
        .attr("name", "googleToken")
        .attr("value", token)
        .appendTo(this);
        
        $(this).find("button[type='submit']").click();
    }
    
    function getRecaptchaToken (e) {
      if ($(this).find("input[name='googleToken']").length == 0) {
        e.preventDefault();
        grecaptcha.execute($('#recaptcha-site-key').val(), {action: 'register'}).then(addRecaptchaToken.bind(this));
      }
    }

    jQuery('body').on('click', '.js-documentation-model', function (e) {
      var $this = $(this);
      var code = $this.data('code');
  
      jQuery.ajax({
        type: "POST",
        async: false,
        url: _base_url + 'endpoints/info_documental',
        data: "code=" + code,
        dataType: 'json',
        success: function (json) {
          if (json.result == 'OK') {
            jQuery('.js-modal-content').html(json.html);
  
            jQuery('.js-modal-content').closest('.modal').find('.js-title').html(json.title);
            
          }
        }
      });
    });


    jQuery('body').on('keyup', '.js-documentation-input', function (e) {
      var $this = $(this);
      if ($this.val().length > 0) {

        clearTimeout(timeout_search);
        timeout_search = setTimeout(function() {
          if (xhr_search && xhr_search.readyState != 4) {
              xhr_search.abort();
          }

          xhr_search = jQuery.ajax({
          type: "POST",
          async: false,
          url: _base_url + 'endpoints/search_products_autocomplete',
          data: "txt=" + $this.val(),
          dataType: 'json',
          success: function (json) {
            if (json.result == 'OK') {
              jQuery('.js-content-search').html(json.html);
            }
          }
        });
        }, 600);
      }
    });

});
