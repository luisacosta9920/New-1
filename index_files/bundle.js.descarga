  checkboxSelectorName: ".js-checkbox",
  checkboxWrapperName: ".js-checkbox-wrapper",
  checkboxHiddenName: ".js-checkbox-hidden",
  extraContentSelectorName: ".js-extracontent",
  extraContentWrapperName: ".js-extracontent-wrapper",
  extraContentHiddenName: ".js-extracontent-hidden",

  /**
   * Extract "GET" parameters from a JS include querystring
   */
  getParams: function(scriptId) {
    var pa = document
      .getElementById(scriptId)
      .src.split("?")
      .pop()
      .split("&");
    var p = {};

    for (var j = 0; j < pa.length; j++) {
      var kv = pa[j].split("=");
      p[kv[0]] = kv[1];
    }

    return p;
  },

  /**
   * Initialize tabs component
   */
  initializeTabs: function() {
    [].slice.call(document.querySelectorAll(".tabs")).forEach(function(el) {
      new CBPFWTabs(el);
    });
  },

  /**
   * Show hidden content on check checkboxes
   */
  initializeCheckboxes: function() {
    jQuery(app.checkboxSelectorName).each(function() {
      jQuery(this)
        .closest(app.checkboxWrapperName)
        .children(app.checkboxHiddenName)
        .toggle(this.checked);
      jQuery(this).click(function() {
        jQuery(this)
          .closest(app.checkboxWrapperName)
          .children(app.checkboxHiddenName)
          .toggle(this.checked);
      });
    });
  },

  /**
   * Extra content
   */
  initializaExtraContent: function() {
    jQuery(app.extraContentSelectorName).each(function() {
      jQuery(this).click(function() {
        jQuery(this)
          .closest(app.extraContentWrapperName)
          .children(app.extraContentHiddenName)
          .toggle(this.checked);
      });
    });
  },

  /**
   * Accordion component
   */
  initializeAccordion: function(openFirst) {
    if (openFirst == true) {
      jQuery(".js-accordion > li:eq(0) .js-accordion-link")
        .addClass("is-active")
        .next()
        .slideDown();
    }

    jQuery(".js-accordion .js-accordion-link").click(function(j) {
      var dropDown = jQuery(this)
        .closest("li")
        .find(".js-accordion-hidden");

      jQuery(this)
        .closest(".js-accordion")
        .find(".js-accordion-hidden")
        .not(dropDown)
        .slideUp();

      if (jQuery(this).hasClass("is-active")) {
        jQuery(this).removeClass("is-active");
      } else {
        jQuery(this)
          .closest(".js-accordion")
          .find(".js-accordion-link.is-active")
          .removeClass("is-active");
        jQuery(this).addClass("is-active");
      }

      dropDown.stop(false, true).slideToggle();

      j.preventDefault();

      $("html,body").animate({ scrollTop: 0 }, "slow");
    });
  },

  /**
   * Loading
   */
  initializeLoading: function() {
    function hideLoading() { 'use strict'
      jQuery(".loading").removeClass("loading--visible");
      setTimeout(function() {
        jQuery(".loading").addClass("loading--invisible");
      }, 2000);
    }

    var tween = new TimelineMax({
      repeat: -1
    });

    tween
      .from(".js-maskline", 2, {
        drawSVG: "0% 0%",
        ease: Power0.easeInOut,
        onComplete: function() {
          jQuery(".js-farma").addClass("visible");
          setTimeout(function() {
            jQuery(".js-soler").addClass("visible");
            setTimeout(function() {
              hideLoading();
            }, 800);
          }, 400);
        }
      })
      .to(".js-maskline", 4, {
        delay: 1,
        drawSVG: "100% 100%",
        ease: Power0.easeInOut
      });
  },

  /**
   * SVG Animation Interest
   */
  initializeInterest: function() {
    var controller = new ScrollMagic.Controller();

    var wipeAnimation = new TimelineMax()
      .fromTo("#tower", 2, { y: "65px", opacity: 0 }, { y: "0px", opacity: 1 })
      .fromTo(
        "#cubes",
        2,
        { x: "-50px", opacity: 0 },
        { x: "15px", opacity: 1 }
      )
      .fromTo(
        "#lateral",
        2,
        { y: "250px", opacity: 0 },
        { y: "161px", opacity: 1 }
      );

    new ScrollMagic.Scene({
      triggerElement: ".js-interest",
      duration: $(window).height(),
      offset: 120,
      triggerHook: 1
