                this.initActions();

            },

            initMarkerCluster: function(){
                var clusterStyles = [
                    {
                        textColor: '#ffffff',
                        url: _base_url+'assets/dist/images/maps/m1.png',
                        height: 30,
                        width: 27
                    },
                    {
                        textColor: 'white',
                        url: _base_url+'assets/dist/images/maps/m2.png',
                        height: 30,
                        width: 27
                    },
                    {
                        textColor: 'white',
                        url: _base_url+'assets/dist/images/maps/m1.png',
                        height: 30,
                        width: 27
                    }
                ];
                _mc_options = {
                    styles: clusterStyles
                };
                _markerCluster = new MarkerClusterer(_map, _markers, _mc_options);
            },

            resetMarkerCluster: function(){
                _markerCluster.clearMarkers();
                _markerCluster.addMarkers(_markers);
            },

            initMarker: function(shop){
                var ref = this;
                
                var latlng = new google.maps.LatLng(shop.lat, shop.lng);

                var marker = new google.maps.Marker({
                    position: latlng,
                    map: _map,
                    title: shop.name,
                    icon: shop.icon,
                    idx: shop.idx,
                    html: getHtmlMap(shop)
                });

                marker.addListener('click', function() {
                    var previousZoom = _map.getZoom();

                    if (_map.getZoom() != shop.zoom) {
                        _map.setZoom(shop.zoom);
                    } else {
                        _info_window.setOptions({ maxWidth: 500 });
                        _info_window.setContent(this.html);
                        _info_window.open(_map, this);
                    }
                    _map.setCenter(marker.getPosition());

                    ref.showShop(_shops[marker.idx]);
                });

                _shops[shop.idx].marker = marker;

                _markers.push(marker);
            },



            showShop: function(shop){
                if (shop.hidden) {
                    $("#shop-"+ shop.id).removeClass("hiddenShop");
                    shop.hidden = false;

                    renderList();
                }

            },
            renderList: function(){
                var evenClass = 'shops-item_even';
                var currentClass = '';

                $(".shops-list__item").each(function(idx, item){
                    if (!$(item).hasClass("hiddenShop")) {

                        if (!currentClass) {
                            $(item).removeClass(evenClass);
                        } else {
                            if (!$(item).hasClass(evenClass)) {
                                $(item).addClass(evenClass);
                            }
                        }

                        currentClass = !currentClass ? 'shops-item_even' : '';
                    }
                });
            },
            loadList: function(noshow){
                this.recalculateDistance();//calcula distancia de cada tienda con respecto a la búsqueda actual y ordena los resultados por distancia
                var $shopsList = $(".shops-list");
                $shopsList.empty();

                _markers = [];

                var numShops = _shops.length;
                var shop;
                var counter = 0;
                var firstShop = null;

                for(var i=0; i<numShops;i++){
                    shop = _shops[i];
                    if (shop.active) {
                        $shopsList.append(this.htmlItemList(shop, counter));
                        console.log('shop list');
                        shop.marker.setMap(_map);
                        _markers.push(shop.marker);
                        counter++;
                        if (!firstShop) {
                            firstShop = shop;
                        }
                    } else{
                        shop.marker.setMap(null);
                    }
                }
                if (!counter){
                    $shopsList.append(this.htmlItemListEmpty());
                } else {

                    this.initActionShops();
                    renderList();
                    if(noshow!=1)
                        checkMapZoom(firstShop, counter);
                }

                this.resetMarkerCluster();
                checkLoadShops();


            },
            htmlItemList: function(shop, idx){
                shop.hidden = idx >= _itemsPage;

                var itemClass = idx >= _itemsPage ? 'hiddenShop' : '';
                var html = '<li id="shop-'+ shop.id +'" data-id="'+ shop.id +'" class="shops-list__item '+ itemClass +'">';
