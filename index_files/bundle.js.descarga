        function isShopInSearch(shop){

            var filterLower = _filterSearch.toLowerCase();
            if (   shop.zipcode == _filterSearch || shop.name.toLowerCase().indexOf(filterLower) > -1 || shop.city.toString().toLowerCase().indexOf(filterLower) > -1 || shop.publicAddress.toLowerCase().indexOf(filterLower) > -1
            ){
