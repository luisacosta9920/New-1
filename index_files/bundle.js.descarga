                                alert('Estamos teniendo problemas con este proceso. Vuelve a intentarlo más tarde.');
                            }
                        });

                }else{
                    if(!validateEmail(_email)){
                        $('.js-pinfo-error').show();
                        $('.js-pinfo-alert').show();
                    }
                    if(!$("#newsletter").find('input[name=legal-privacy]').is(":checked")){
                        $('.js-pinfo-policy').show();
                        $('.js-pinfo-alert').show();
                    }
                    if(!$("#newsletter").find('input[name=legal-legal]').is(":checked")){
