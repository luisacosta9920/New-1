// Import CSS breakpoints
var breakpoint = {};
breakpoint.refreshValue = function () {
  this.value = window
    .getComputedStyle(document.querySelector("body"), ":after")
    .getPropertyValue("content")
    .replace(/\"/g, "");
};

// Window scroll
function windowScrollHeader() {
  if (
    breakpoint.value != "xxs" &&
    breakpoint.value != "xs" &&
    breakpoint.value != "sm"
  ) {
    if (jQuery(window).scrollTop() > 80) {
      jQuery(".js-main-header").addClass("is-mini");
      jQuery("body").addClass("is-header-mini");
    } else {
      jQuery(".js-main-header").removeClass("is-mini");
      jQuery("body").removeClass("is-header-mini");
    }
  }
}
// END Window scroll

// Main Nav
function clearMenus(e) {
  $(".backdrop").remove();
  navHide();
}

var navIsHover = false;
var subnavIsHover = false;
var subnavIsVisible = false;
var subnavInsideNav = true;

function updateQueryStringParameter(uri, key, value) {
  var re = new RegExp("([?&])" + key + "=.*?(&|$)", "i");
  var separator = uri.indexOf('?') !== -1 ? "&" : "?";
  if (uri.match(re)) {
    return uri.replace(re, '$1' + key + "=" + value + '$2');
  } else {
    return uri + separator + key + "=" + value;
  }
}

function navShow(item) {
  var $subnav = item.closest(".js-nav-item").find(".js-subnav");
  if ($subnav.length > 0) {
    if (
      Modernizr.touchevents &&
      item.closest(".js-nav-item").hasClass("is-hover")
    ) {
      $(".js-nav-item").removeClass("is-hover");
      $subnav.removeClass("is-active");
      $subnav.removeClass("is-hover");
      $(".js-subnav").removeClass("is-active");

      $(".topbar").click();

      if (!subnavInsideNav) $(".js-subnav").removeClass("is-active");
    } else {
      subnavIsVisible = true;
      $(".js-main-header-wrap").addClass("is-subnav-visible");
      $(".js-nav-item").removeClass("is-hover");
      item.closest(".js-nav-item").addClass("is-hover");

      // Touch backdrop
      if (Modernizr.touchevents) {
        $(".backdrop").remove();
        $(document.createElement("div"))
          .addClass("backdrop")
          .insertAfter(item)
          .on("click", clearMenus);
        $(".topbar").click();
      }

      if (!subnavInsideNav) $(".js-subnav").removeClass("is-active");
      $subnav.addClass("is-active");
    }
  } else {
    if (Modernizr.touchevents) {
      $(".js-nav-item").removeClass("is-hover");
      if (!subnavInsideNav) $(".js-subnav").removeClass("is-active");
    } else {
      $(".js-nav-item").removeClass("is-hover");
      if (!subnavInsideNav) $(".js-subnav").removeClass("is-active");
    }
  }
}

function navHide() {
  $(".backdrop").remove();
  if (!navIsHover && !subnavIsHover) {
    subnavIsVisible = false;
    $(".js-main-header-wrap").removeClass("is-subnav-visible");
    $(".js-nav-item").removeClass("is-hover");
    setTimeout(function () {
      if (!subnavInsideNav) {
        if (!navIsHover && !subnavIsHover)
          $(".js-subnav").removeClass("is-active");
      }
    }, 110);
  }
}

function navHover() {
  navShow($(this));
}

function navOut() {
  navHide();
}

function subnavHover() {
  subnavIsHover = true;
}

function subnavOut() {
  setTimeout(function () {
    subnavIsHover = false;
    navHide();
  }, 110);
}

var mainNav = function () {
  if (!Modernizr.touchevents) {
    $(".js-nav-link").hoverIntent(navHover, navOut);
    $(".js-nav-link").hover(
      function () {
        navIsHover = true;
      },
      function () {
        navIsHover = false;
      }
    );
    $(".js-nav").mouseleave(function () {
      navHide();
    });
    $(".js-subnav").hover(subnavHover, subnavOut);
  } else {
    $(".js-nav-link").on("click", function (e) {
      e.preventDefault();
      navShow($(this));
    });
  }
};

// Window Resize
var windowResize = function () {
  // Update current breakpoint
  breakpoint.refreshValue();
};
// END Window Resize

// Nav mobile
function toggleNavMobile() {
  jQuery("body").toggleClass("is-nav-mobile-open");
}

// Main Nav Mobile
var navMobileIsOpen = false;
var scrollPos = 0;

var mainNavMobileToggle = function () {
  navMobileIsOpen = !navMobileIsOpen;
  $("body").toggleClass("is-nav-mobile-open");
};

function checkNavMobileBackdropClick(e) {
  if (navMobileIsOpen && $(e.target).hasClass("js-nav-mobile-backdrop")) {
    e.preventDefault();
    mainNavMobileToggle();
  }
}

var mainNavMobile = function () {
  if ($("body").hasClass("is-nav-mobile-open")) navMobileIsOpen = true;

  $(".js-hamburger").on("click", function () {
    mainNavMobileToggle();
  });

  $(".js-nav-mobile-link").on("click", function (e) {
    var $item = $(this).closest(".js-nav-mobile-item");
    var $subnav = $item.find(".js-subnav-mobile");

    if ($subnav.length > 0) {
      e.preventDefault();

      if ($item.hasClass("is-open") || $item.hasClass("is-open-init")) {
        $subnav.slideUp(250, function () {
          $item.removeClass("is-open is-open-init");
        });
      } else {
        //$('.js-nav-mobile-item').removeClass('is-open is-open-init');
        // $('.js-subnav-mobile').hide();
        $item.addClass("is-open");
        $subnav.slideDown(250);
      }
    }
  });

  $(".js-subnav-mobile-link").on("click", function (e) {
    var $item = $(this).closest(".js-subnav-mobile-item");
    var $subnav = $item.find(".js-subsubnav-mobile");

    if ($subnav.length > 0) {
      e.preventDefault();

      if ($item.hasClass("is-open") || $item.hasClass("is-open-init")) {
        $subnav.slideUp(250, function () {
          $item.removeClass("is-open is-open-init");
        });
      } else {
        //$('.js-nav-mobile-item').removeClass('is-open is-open-init');
        // $('.js-subnav-mobile').hide();
        $item.addClass("is-open");
        $subnav.slideDown(250);
      }
    }
  });

  document.addEventListener(
    "click",
    function (e) {
      checkNavMobileBackdropClick(e);
    },
    true
  );

  document.addEventListener(
    "touchend",
    function (e) {
      checkNavMobileBackdropClick(e);
    },
    true
  );
};
// END Nav mobile

// Know if element is visible on screen
function onScreen() {
  var scrollTop = jQuery(window).scrollTop();
  var scrollTopBottom =
    jQuery(window).scrollTop() + document.documentElement.clientHeight;

  // jQuery('.lucia').each(function(){
  //     var elementTop = jQuery(this).offset().top;

  //     if (elementTop > scrollTop && elementTop < scrollTopBottom) {
  //         jQuery(this).addClass('visible');
  //     }
  //     else {
  //         jQuery(this).removeClass('visible');
  //     }
  // });

  jQuery(".lucia").each(function () {
    var elementTop = jQuery(this).offset().top;

    if (elementTop < scrollTopBottom) {
      jQuery(this).addClass("visible");
    }
  });
}
// END - Know if element is visible on screen

// Smooth scroll
var smoothScroll = function () {
  $(".js-smooth-scroll").on("click", function (e) {
    var $target = $($(this).attr("data-href"));
    if ($target.length === 0) $target = $($(this).attr("href"));

    if ($target.length > 0) {
      e.preventDefault();
      var bodyTop = $target.offset().top - $(".js-main-header").outerHeight();

      $("html, body").animate({
        scrollTop: bodyTop
      }, 400);
    }
  });
};

// Product environment graphs
function productGraphs() {
  var graphs = [];
  var graphs_ids = [];

  $(".js-graph-benefits").each(function (index, val) {
    var id = $(this).attr("id");
    var value = $(this)
      .find(".js-graph-value")
      .text();
    var text = $(this)
      .find(".js-graph-text")
      .text();
    var colorBg = "#eee";
    var colorMain = "#D6541B";

    if ($(this).hasClass("is-blue")) colorMain = "#26BEFF";
    if ($(this).hasClass("is-green")) colorMain = "#93DC59";
    if ($(this).hasClass("is-dark-blue")) colorMain = "#8C9EFF";

    graphs_ids[index] = id;

    graphs[index] = Circles.create({
      id: id,
      radius: 112,
      value: value,
      maxValue: 100,
      width: 8,
      text: function (value) {
        return (
          '<div class="benefits__graph__content"><span class="benefits__graph__percent">' +
          value +
          '%</span><span class="benefits__graph__desc">' +
          text +
          "</span></div>"
        );
      },
      colors: [colorBg, colorMain],
      duration: 1000,
      wrpClass: "circles-wrp",
      textClass: "",
      valueStrokeClass: "circles-valueStroke",
      maxValueStrokeClass: "circles-maxValueStroke",
      styleWrapper: true,
      styleText: false
    });

    // Forzamos el redimensionado de las gráficas
    for (var i = 0; i < graphs.length; i++) {
      graphs[i].updateRadius(
        Math.round(
          ($("#" + graphs_ids[i])
            .closest(".benefits__graph")
            .width() -
            2) /
          2
        )
      );
    }
  });

  $(window).resize(function () {
    for (var i = 0; i < graphs.length; i++) {
      graphs[i].updateRadius(
        Math.round(
          ($("#" + graphs_ids[i])
            .closest(".benefits__graph")
            .width() -
            2) /
          2
        )
      );
    }
  });
}

function setCookie(cname, cvalue, exdays) {
  var d = new Date();
  d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
  var expires = "expires=" + d.toUTCString();
  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}

function getCookie(cname) {
  var name = cname + "=";
  var ca = document.cookie.split(';');
  for (var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}


